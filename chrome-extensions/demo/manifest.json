{
  "name": "regulex",
  "manifest_version": 2,
  "version": "1.0.0",
  "description": "regulex",
  "browser_action": {
    "default_icon": "icon.ico",
    "default_title": "chrome-extensions-demo",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "contextMenus",
    "cookies",
    "notifications",
    "activeTab",
    "storage",
    "unlimitedStorage",
    "<all_urls>",
    "chromewebdata"
  ]
}
